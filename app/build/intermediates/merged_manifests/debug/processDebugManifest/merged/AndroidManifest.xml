        android:icon="@mipmap/ic_launcher_nba_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_nba_logo_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.arlinda.nbaretrofit.control.Main3Activity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.arlinda.nbaretrofit.control.ListaJogadoresActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>